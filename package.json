    "helmet": "^3.21.2",
    "knex": "^0.20.10",
    "morgan": "^1.9.1",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "postgrator-cli": "^3.3.0",
    "supertest": "^4.0.2"
  }
